<!DOCTYPE html>
<html>

<h1>Edit photo</h1>

<form action="/update_photo/57">
  <div>
    <label for="photo_caption">Caption:</label>
    <input id="<%= @photo_caption %>" type="<%= @photo_caption %>" name="the_caption" value="<%= @photo_caption %>">
  </div>
  <div>
    <label for="photo_image_url">Image URL:</label>
    <input id="photo_image_url" type="text" name="<%= @photo_source %>" value="<%= @photo_source %>">
  </div>
  <div>
    <button>Update Photo</button>
  </div>
</form>


</html>
